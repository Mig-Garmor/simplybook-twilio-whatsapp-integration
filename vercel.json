{
  "functions": {
    "api/handler.js": {
      "runtime": "edge"
    },
    "api/recurring.js": {
      "runtime": "edge"
    }
  },
  "crons": [
    {
      "path": "/api/recurring.js",
      "schedule": "7 9 * * *"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/handler.js"
    }
  ]
}
